{% block title %}Hello AnalyticsController!{% endblock %}

{% block body %}
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
    {{ gc_draw(area, 'area') }}
    {{ gc_draw(col, 'col') }}
